        <h2>GROCERY STORE</h2>
        <h4>CUSTOMER DETAILS</h4>
        <br><br>

        <div class="container">
            <div class="row">
                <div class="col">
                    <table>
                    
                        <th>CUSTOMER ID</th>
                        <th>NAME</th>
                        <th>PHONE</th>
                        <th>EMAIL</th>
                        <% for(var i=0; i < arr.length;i++ ){ %>
                            <tr>
                                <td>
                                    <%= arr[i].cust_id %>
                                </td>
                                <td>
                                    <%= arr[i].name %>
                                </td>
                                <td>
                                    <%= arr[i].phone %>
                                </td>
                                <td>
                                    <%= arr[i].email %>
                                </td>
                            </tr>
                            <% } %>
                    </table>
                </div>

                <div class="col">
                    <form method="post">
                        <h3>CUSTOMER Details</h3>
                        <label for="name" class="form-label">CUSTOMER Name</label>
                        <input type="text" name="name" id="" class="form-control" autocomplete="off">
                    
                      
                        <label for="phone" class="form-label">Phone number</label>
                        <input type="text" name="phone" id="" class="form-control" autocomplete="off">
                    

                        <label for="email" class="form-label">EMAIL</label>
                        <input type="email" name="email" id="" class="form-control" autocomplete="off">
                    
                        <!-- <label for="datejoined">Joining Date</label>
